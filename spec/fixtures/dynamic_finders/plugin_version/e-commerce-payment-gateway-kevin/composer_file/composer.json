{
    "name": "getkevin/kevin-woocommerce",
    "description": "E-Commerce payment gateway plugin for kevin. client.",
    "type": "wordpress-plugin",
    "version": "3.2.2",
    "license": "MIT",
    "authors": [
        {
            "name": "kevin.",
            "email": "info@kevin.eu"
        }
    ],
    "support": {
        "email": "support@kevin.eu"
