    "@wordpress/i18n": "^4.2.4",
    "@wordpress/icons": "^6.1.1",
    "@wordpress/keycodes": "^3.2.4",
    "@wordpress/plugins": "^4.4.0",
    "@wordpress/primitives": "^3.9.0",
    "@wordpress/scripts": "^21.0.1",
