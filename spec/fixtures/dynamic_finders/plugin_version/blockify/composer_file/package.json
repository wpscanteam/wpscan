    "oslllo-svg-fixer": "^2.1.1",
    "postcss-preset-env": "^7.4.3",
    "react-google-autocomplete": "^2.6.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "run-for-every-file": "^1.1.0",
    "style-object-to-css-string": "^1.0.1",
